javascript:(function(){function onVideoEnd(){fetch('http://localhost:5000/video-ended');}function addVideoEndListener(){const video=document.querySelector('video');if(video){video.addEventListener('ended',onVideoEnd);console.log('Listener added to video');}else{console.log('No video element found');}}const observer=new MutationObserver((mutations)=>{mutations.forEach((mutation)=>{if(mutation.addedNodes.length){addVideoEndListener();}});});observer.observe(document.body,{childList:true,subtree:true});addVideoEndListener();})();
